echo
echo Credenciales actuales
echo ---------------------
git config --global user.name
git config --global user.email
echo
echo Managers de credenciales actuales
echo ------------------------------
echo -n 'global: ';git config --global credential.helper;echo
echo -n 'system: ';git config --system credential.helper;echo
echo -n 'workspace: ';git config credential.helper;echo
echo
echo Remotos actuales
echo ----------------
git remote -v
echo
